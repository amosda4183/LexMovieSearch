<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.atlbr.lexmoviesearch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/film_roll"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowtimeSearch" />
        <activity android:name=".TheaterSearch" />
        <activity android:name=".TitleSearch" />
        <activity android:name=".SearchResults" />
        <activity android:name=".ConcessionChoice" />
        <activity android:name=".CinemarkMenu" />
        <activity android:name=".RegalMenu" />
        <activity android:name=".TavernMenu"></activity>
    </application>

</manifest>